{
  "name": "stl2d",
  "version": "1.0.0",
  "description": "STL to 2D Drawing Projections Viewer",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nanoid": "^5.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.0",
    "wouter": "^3.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "lucide-react": "^0.344.0",
    "@tailwindcss/postcss": "^4.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=22.13.0",
    "yarn": ">=1.22.0"
  }
}
